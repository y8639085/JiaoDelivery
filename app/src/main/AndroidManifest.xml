<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unnc.zy18717.jiaodelivery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".AllItemsActivity"></activity>
        <activity android:name=".StatisticsActivity"></activity>
        <provider
            android:authorities="com.unnc.zy18717.jiaodelivery.MyProvider"
            android:multiprocess="true"
            android:exported="true"
            android:name="com.unnc.zy18717.jiaodelivery.MyProvider">
        </provider>
    </application>

</manifest>